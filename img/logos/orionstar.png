    function modal_form_saveinfop() {
        let index = loading(); //0代表加载的风格，支持0-2
        $('#modal_submit').attr('disabled', true);
        const isFillInContent = getIsFillInContent(formdataArray);
        // if (currentPage === 'home_float') {
