	var lastTop = 0;
	$(window).scroll(function() {
		if($(".nav-bg").css("display")!="none"){
			$('header').slideDown();
			return;
		}
		var scrollTop = $(this).scrollTop();
		if(Math.abs(scrollTop-lastTop)>20){
			if(scrollTop > lastTop){
				$('header').slideUp();
				$(".header-bg").slideUp();
			}else{
				$('header').slideDown();
				$(".header-bg").slideDown();
			}
		}
		console.log(scrollTop-lastTop);
		lastTop = scrollTop;
	});



